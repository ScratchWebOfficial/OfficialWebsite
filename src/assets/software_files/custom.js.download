    // Bind keyup event on the input
    $('#searchSoft1').keyup(function() {
        // If value is not empty
        if ($(this).val().length == 0) {
            // Hide the element
            $('#searchSoft1Result').hide();
            $('#search-btn-banner').show(); 
            $('#close-btn-banner').hide();
        } else {
            // Otherwise show it
            $('#searchSoft1Result').show();
            $('#close-btn-banner').show();
            $('#search-btn-banner').hide(); 
        }
    }).keyup();

    // Bind keyup event on the input
    $('#searchSoft2').keyup(function() {
        // If value is not empty
        if ($(this).val().length == 0) {
            // Hide the element
            $('#search-btn-2').show();
            $('#close-btn-2').hide();
            $('#allsoftdiv').show();
            $('#searchrightsoftdiv').hide();
        } else {
            // Otherwise show it
            $('#close-btn-2').show();
            $('#search-btn-2').hide();
            $('#allsoftdiv').hide();
            $('#searchrightsoftdiv').show();
        }
    }).keyup();

    $(document).ready(function(){
        $('#searchSoft1').keyup(function(){
            var searchSoft1 = $('#searchSoft1').val();
            $.ajax({
