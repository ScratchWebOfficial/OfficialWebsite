    function valuereset1() {
        var inputvalue=document.getElementById('searchSoft1').value;
        if(inputvalue=="") {
            document.getElementById('searchSoft1').focus();
        } else {
            document.getElementById('searchSoft1Result').style.display='none';
            document.getElementById('searchSoft1').value="";
            document.getElementById('searchSoft1').blur();
            document.getElementById('close-btn-banner').style.display='none';
