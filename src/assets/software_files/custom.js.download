//  file selection checking for FORM type 2
    function processdocFile2() {
        var fileName = document.getElementById('custfile2').value;
        var idxDot = fileName.lastIndexOf(".") + 1;
        var extFile = fileName.substr(idxDot, fileName.length).toLowerCase();
        if (extFile=="doc" ||extFile=="docx" || extFile=="pdf" ){
            document.getElementById('custfile2span').innerHTML='';
            var size = parseFloat(document.getElementById('custfile2').files[0].size / 1024).toFixed(2);
            if(size>2048) {
                document.getElementById('custfile2').value='';
                document.getElementById('custfile2span').innerHTML='File size must be within 2MB.';
            }
        } else {
            document.getElementById('custfile2').value='';
            document.getElementById('custfile2span').innerHTML='Only pdf/doc/docx file is allowed.';
        }   
    }




